{
  "name": "tab-ext-einstein-scenarios",
  "version": "0.3.1",
  "description": "A Tableau extension that allows \"what if\" style scenario analysis powered by Einstein Discovery's predictive models.",
  "main": "index.js",
  "repository": "git@github.com:jhegele/tab-einstein-scenarios.git",
  "author": "John Hegele",
  "license": "MIT",
  "scripts": {
    "py:make-requirements": "pip freeze > ./app/requirements.txt",
    "ts:check-types": "npx tsc --noEmit --skipLibCheck",
    "dev": "webpack --config webpack.config.dev.js --display-error-details",
    "prod": "yarn py:make-requirements && yarn ts:check-types && webpack --config webpack.config.prod.js",
    "docs": "npx docsify serve docs",
    "docker:build": "docker build -t einstein-scenarios .",
    "docker:test": "docker run -e ENV -e FLASK_SECRET_KEY -e PUSHER_SECRET -e SFDC_CONSUMER_KEY -e SFDC_CONSUMER_SECRET -e PORT=5000 -d -p 5000:5000 einstein-scenarios"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@reduxjs/toolkit": "^1.3.6",
    "@tableau/tableau-ui": "^3.0.0",
    "@tippyjs/react": "^4.0.2",
    "axios": "^0.19.2",
    "lodash": "^4.17.19",
    "numeral": "^2.0.6",
    "pusher-js": "^6.0.3",
    "react": "^16.13.1",
    "react-color": "^2.18.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "string-similarity": "^4.0.1",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@tableau/extensions-api-types": "^1.4.0",
    "@types/lodash": "^4.14.152",
    "@types/numeral": "^0.0.28",
    "@types/react": "^16.9.35",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-logger": "^3.0.7",
    "@types/string-similarity": "^3.0.0",
    "@types/uuid": "^8.0.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "docsify-cli": "^4.4.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "style-loader": "^1.2.1",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  }
}
